package ExampleArray;

public class Univ {
	public static void main(String[] args) {
		String [] a = {"Les", "Daisy", "Sam", "Mastercat", "Fudeg", "Luci", "Harry", "Bas", "Esmerelda", "Blonkie", "King", "Bird", "Audrey", "Fay"};
		a[13] = "Lydia";
		System.out.println(a);
		for(int i = 0;i<= a.length; i++) {
			System.out.println(a);
		}
	}
}
