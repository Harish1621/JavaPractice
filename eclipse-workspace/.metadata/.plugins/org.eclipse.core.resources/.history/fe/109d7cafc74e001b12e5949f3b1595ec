package Lexington;


public class Tenents {
	String name;
	String companyName;
	String position;
	String job;
	int exp;
	
	public Tenents(String n, String cn, String p, int e) {
		name =n;
		companyName = cn;
		position = p;
		exp = e;
		
	}
	enum jobType {
		FTE,
		COC
	}
	public void jobType (String j) {
		job = j;
	}
}
