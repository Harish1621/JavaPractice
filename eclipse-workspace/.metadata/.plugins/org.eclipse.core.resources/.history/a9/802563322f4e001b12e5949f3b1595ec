package Example;

public class Addition {

	public static void main(String[] args) {
		int n = Integer.parseInt(args[0]);
		
		Addition a = new Addition();
		System.out.println(a.Add(n));
	}
	
	public int Add(int n) {
		int sum =0;
		for(int i = 1; i<=n; i++) {
			/* System.out.print(i + " "); */
			
			System.out.println();
			sum = sum+i;
			System.out.println(sum);
			for(int j =1; j<=i;j++) {
				/* System.out.println("+"); */
				sum = i+j;
				/* System.out.print(j + " "); */
			System.out.println(sum);
				/* System.out.println(i + "+" + ++i); */
			
		}
		
	}
		return n;
}
}