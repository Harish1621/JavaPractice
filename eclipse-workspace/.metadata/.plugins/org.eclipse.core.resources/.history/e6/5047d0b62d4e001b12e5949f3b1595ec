package Example;

public class Addition {

	public static void main(String[] args) {
		int n = Integer.parseInt(args[0]);
		
		Addition a = new Addition();
		System.out.println(a.Add(n));
	}
	
	public int Add(int n) {
		for(int i = 0; i<=n; i++) {
			System.out.print(i + " ");
			System.out.println();
			for(int j =0; j<=i;j++) {
			
			System.out.print(j + " ");
				/* System.out.println(i + "+" + ++i); */
			
		}
		
	}
		return n;
}
}