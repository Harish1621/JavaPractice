package Example;

public class Armstrong {
	
	public static void main(String[] args) {
		int n,a=0,z,b, x=0,temp;
		n = Integer.parseInt(args[0]);
		temp = n;
		while(temp > 0) {
		z =temp%10;
		temp = temp/10;
		a = a+(z*z*z);
		
		}
		
		if (a == n) {
			System.out.println("Given number is an Armstrong " + args[0]);
		} else {
			System.out.println("Given number is not an Armstrong " + args[0]);
		}
	}
}
