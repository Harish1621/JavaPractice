package Example;

public class Addition {

	public static void main(String[] args) {
		int n = Integer.parseInt(args[0]);
		
		Addition a = new Addition();
		
	}
	
	public void Add(int n) {
		for(int i = 0; i<=n; i++) {
			if(i<=n) {
				System.out.println(i);
			} else {
				System.out.println(n);
			}
		}
		
	}
}