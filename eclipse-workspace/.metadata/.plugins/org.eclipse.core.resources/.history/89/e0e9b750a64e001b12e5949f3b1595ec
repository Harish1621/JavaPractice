package Lexington;

public class FirmsMain {

	public static void main(String[] args) {
		
		Tenents r = new Tenents("Raj", "Microsoft", "FTE", "SD2", 230000);
		Tenents s = new Tenents("Sai", "Microsoft", "FTE", "SD1", 210000);
		Tenents c = new Tenents("C.Manoj", "Amazon", "FTE", "SD1", 190000);
		Tenents b = new Tenents("B.Manoj", "T-Mobile", "FTE", "SA3", 130000);
		Tenents v = new Tenents("Vikram", "7/11", "FTE", "SD2", 140000);
		Tenents h = new Tenents("Harish", "T-Mobile", "COC", "SA2", 100000);
		
		TenentsPositions tp = new TenentsPositions();
		
		tp.jobType(r.name, r.position);
		tp.jobType(s.name, s.position);
		tp.jobType(c.name, c.position);
		tp.jobType(b.name, b.position);
		tp.jobType(v.name, v.position);
		tp.jobType(h.name, h.position);
		
		tp.costlyTenent(r.name, r.salary);
		tp.costlyTenent(s.name, s.salary);
		tp.costlyTenent(c.name, c.salary);
		tp.costlyTenent(b.name, b.salary);
		tp.costlyTenent(v.name, v.salary);
		tp.costlyTenent(h.name, h.salary);
		
		tp.avgTenent(r.name, r.salary);
		tp.avgTenent(s.name, s.salary);
		tp.avgTenent(c.name, c.salary);
		tp.avgTenent(b.name, b.salary);
		tp.avgTenent(v.name, v.salary);
		tp.avgTenent(h.name, h.salary);
		
	}

}
